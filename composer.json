{
	"name": "lalop/aphet",
	"require": {
		"php": ">=5.4",
		"symfony/console": "~2.0",
		"leafo/scssphp": "~0.3",
		"kriswallsmith/assetic": "~1.0",
		"natxet/CssMin": "~3.0",
		"mrclay/minify": "2.*"
	},
    "autoload": {
        "psr-4": {
            "Aphet\\": "src"
        }
    },
    "suggest": {
        "leafo/scssphp-compass": "Aphet provides the integration with the SCSS compass plugin"
    },
    "require-dev": {
        "mikey179/vfsStream": "v1.2.0",
        "phpunit/phpunit": "3.*"
    }
}
